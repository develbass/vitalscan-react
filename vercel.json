{
  "version": 2,
  "buildCommand": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=4096' yarn build",
  "outputDirectory": "dist",
  "installCommand": "yarn install --frozen-lockfile",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
